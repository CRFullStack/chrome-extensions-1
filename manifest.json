{
  "name": "Beautiful New Tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get beautiful images with quotes whenever you open a new tab.",
  "homepage_url": "http://codesparta.com",
  "icons": {
    "16": "images/poop_16.png",
    "48": "images/poop_48.png",
    "128": "images/poop_128.png"
  },

  "chrome_url_overrides": {
    "newtab": "HTML/override.html"
  },

  "permissions": [
    "https://source.unsplash.com/",
    "http://quotes.rest/",
    "bookmarks"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
